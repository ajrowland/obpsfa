---
import ExtendedImage from "./ExtendedImage.astro";
import FixtureList from "./FixtureList.astro";
import Gallery from "./Gallery.astro";
import Table from "./Table.astro";
import Missing from "./Missing.astro";

interface Serializers {
  [key: string]: any;
}

const serializers: Serializers = {
  extendedImage: ExtendedImage,
  fixtureList: FixtureList,
  gallery: Gallery,
  table: Table,
};

const Cmp = serializers[Astro.props.node._type]
  ? serializers[Astro.props.node._type]
  : Missing;
---

<Cmp {...Astro.props.node} />
